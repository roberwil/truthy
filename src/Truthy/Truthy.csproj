<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <LangVersion>10</LangVersion>
        <Title>Truthy</Title>
		<PackageReadmeFile>nuget.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
        <Authors>roberwil</Authors>
        <Description>
			Create a truth table and evaluate it with your inputs, Truthy figures out the formula; 
			Perform your logical operations plus the 'special or', which allows you to check if a terms is equal to any other terms.
		</Description>
        <Copyright>Copyright Â© Roberto W. P. Ribeiro, 2023.</Copyright>
        <RepositoryUrl>https://github.com/roberwil/truthy</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>booleans, truth table, logical operations, bool</PackageTags>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="../../readmes/nuget.md" Pack="true" PackagePath="/" />
		<None Include="../../LICENSE" Pack="true" PackagePath="/" />
	</ItemGroup>
	
</Project>
